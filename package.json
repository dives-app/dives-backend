{
  "name": "dives-backend",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "test": "jest",
    "debug": "SLS_DEBUG=* serverless offline",
    "serve": "serverless offline",
    "dev": "npm run dev:db && npm run dev:backend & npm run watch",
    "dev:backend": "nodemon --exec serverless offline",
    "dev:db": "docker-compose -f docker-compose-dev.yml up -d"
  },
  "dependencies": {
    "@types/graphql": "14.5.0",
    "apollo-server-lambda": "^3.0.0-alpha.3",
    "apollo-server-plugin-http-headers": "^0.1.3",
    "argon2": "^0.27.0",
    "aws-sdk": "^2.750.0",
    "bufferutil": "4.0.1",
    "canvas": "2.6.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "type-graphql": "^1.0.0",
    "typeorm": "^0.2.26",
    "utf-8-validate": "5.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.62",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.10.0",
    "apollo-server-testing": "^3.0.0-alpha.3",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "2.1.1",
    "serverless": "^2.0.0",
    "serverless-offline": "^6.7.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  },
  "resolutions": {
    "graphql": "15.3.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
